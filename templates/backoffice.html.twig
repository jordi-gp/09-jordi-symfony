<!doctype html>
<html lang="ca">
<head>
    <title>{% block title %}Apartat Backoffce{% endblock %}</title>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    {% block stylesheet %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}
    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}
</head>
<body>
{% block body %}
    <header>
        {% include 'backoffice/_menu.html.twig' %}
    </header>
    <main>
        {% block main %}
        {% endblock %}
    </main>
{% endblock %}
</body>

